{
  "expo": {
    "name": "fell-app",
    "slug": "fell-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "fellapp",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,

    "ios": {
      "supportsTablet": true
    },

    "androidNavigationBar": {
      "backgroundColor": "#111",
      "barStyle": "light-content"
    },

    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#111"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.fellapp"
    },

    "web": {
      "output": "static"
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",

          "backgroundColor": "#111",
          "dark": {
            "backgroundColor": "#111"
          }
        }
      ],
      "expo-sqlite"
    ],

    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
